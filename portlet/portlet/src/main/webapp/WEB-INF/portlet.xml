<?xml version="1.0" encoding="UTF-8"?>
<portlet-app
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
  
  <portlet>
    
    <description>Dysoweb Portlet</description>
    <portlet-name>DysowebPortlet</portlet-name>
    <display-name>DysowebPortlet</display-name>
    <portlet-class>com.requea.dysoweb.portlet.DysowebPortlet</portlet-class>
    
    <expiration-cache>-1</expiration-cache>
    
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
      <portlet-mode>EDIT</portlet-mode>
      <portlet-mode>HELP</portlet-mode>
    </supports>
    
    <supported-locale>en</supported-locale>        
    
    <portlet-info>
      <title>DysowebPortlet</title>
      <short-title>Dysoweb Portlet</short-title>
      <keywords>Requea,Dysoweb,Portlet</keywords>
    </portlet-info>

  </portlet>
  
</portlet-app>


